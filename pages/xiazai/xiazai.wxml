<!--pages/xiazai/xiazai.wxml-->
<swiper indicator-dots="{{indicatorDots}}" indicator-active-color="#000" circular="true" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="355" height="150" />
    </swiper-item>
  </block>
</swiper>

<view class="button" bindtap="tanchuang">
  <button>下载</button>
</view>